{
  "catalog": {
    "name": "T-Dataiku",
    "description": "Dataiku by T",
    "version": "0.2",
    "distroid": "t-dataiku",
    "categories": [
      "Data Science"
    ]
  },
  "services": [
    {
      "srvcid": "ssh",
      "name": "SSH",
      "port": "22",
      "scheme": "",
      "isdash": false,
      "path": "",
      "sysctl": "",
      "authtoken": false,
      "loadbalanced": false
    },
    {
      "srvcid": "dataiku-web",
      "name": "DataIKU-web",
      "port": "10000",
      "scheme": "http",
      "isdash": true,
      "path": "/",
      "sysctl": "",
      "authtoken": false,
      "loadbalanced": false
    }
  ],
  "roles": [
    {
      "id": "dss-server",
      "name": "DSS-server",
      "min_cores": 2,
      "min_memory": 4096,
      "cardinality": "1",
      "anti_affinity_group_id": false,
      "image": {}
    }
  ],
  "clusterconfig": [
    {
      "id": "default",
      "type": "default",
      "role_services": {
        "DSS-server": [
          "SSH",
          "DataIKU-web"
        ]
      }
    }
  ],
  "appconfig": {
    "basedir": "appconfig"
  },
  "document": {
    "filepath": "documentation.md"
  },
  "logo": {
    "filepath": "logo.png"
  },
  "image": {
    "build": {
      "basedir": "image",
      "repotag": "test/dataiku-t:0.2",
      "os": "centos7"
    }
  }
}